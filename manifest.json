{
  "manifest_version": 3,
  "name": "FB Access Extractor",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "cookies",
    "tabs"
  ],
  "host_permissions": [
    "https://*.facebook.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.facebook.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}